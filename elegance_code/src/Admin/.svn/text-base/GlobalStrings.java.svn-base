package Admin;
/**
 * This class contains some globally static strings.
 *
 * @author zndavid
 * @version 1.0
 */
public class GlobalStrings
{
	/** The string for Continuous relations */
	public static final String CONTINUOUS = "Continuous";

	/** The string for Presynaptic relations */
	public static final String PRESYNAPTIC = "Presynaptic";

	/** The string for postsynaptic relations */
	public static final String POSTSYNAPTIC = "Postsynaptic";

	/** The string for multiple presynaptic relations */
	public static final String MULTIPLE_PRESYNAPTIC = "Multiple presynaptic";

	/** The string for multiple postsynaptic relations */
	public static final String MULTIPLE_POSTSYNAPTIC = "Multiple postsynaptic";

	/** The string for gap junctions */
	public static final String GAP_JUNCTION = "Gap junction";

	/** The string for uncharecterized relations */
	public static final String UNCHARECTERIZED = "Uncharacterized";

	/** A string array of all the relation types */
	public static final String [] relationTypes =
	{
		CONTINUOUS,
		PRESYNAPTIC,
		MULTIPLE_PRESYNAPTIC,
		GAP_JUNCTION
	};

	/**
	 * The private constructor so that this class is never instantiated
	 */
	private GlobalStrings (  ) {}
}
